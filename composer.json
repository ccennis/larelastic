{
    "name": "ink-empire/elastic",
    "description": "Generic Wrapper for elasticsearch and indexing",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "laravel/scout": "^8.0|^9.0",
        "elasticsearch/elasticsearch": "8.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": { "Larelastic\\Elastic\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Larelastic\\Elastic\\Tests\\": "tests/" }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}